<?xml version="1.0" standalone="yes"?>
<xs:schema id="DataSet1" targetNamespace="http://www.tempuri.org/DataSet1.xsd" xmlns:mstns="http://www.tempuri.org/DataSet1.xsd" xmlns="http://www.tempuri.org/DataSet1.xsd" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata" attributeFormDefault="qualified" elementFormDefault="qualified">
  <xs:element name="DataSet1" msdata:IsDataSet="true" msdata:Locale="zh-CN">
    <xs:complexType>
      <xs:choice maxOccurs="unbounded">
        <xs:element name="scores">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="stu_id" type="xs:string" />
              <xs:element name="stu_name" type="xs:string" minOccurs="0" />
              <xs:element name="english_score" type="xs:double" minOccurs="0" />
              <xs:element name="math_score" type="xs:double" minOccurs="0" />
              <xs:element name="computer_score" type="xs:double" minOccurs="0" />
              <xs:element name="physics_score" type="xs:double" minOccurs="0" />
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:choice>
    </xs:complexType>
    <xs:unique name="Constraint1" msdata:PrimaryKey="true">
      <xs:selector xpath=".//mstns:scores" />
      <xs:field xpath="mstns:stu_id" />
    </xs:unique>
  </xs:element>
</xs:schema>