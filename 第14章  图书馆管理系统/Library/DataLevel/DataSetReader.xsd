<?xml version="1.0" standalone="yes"?>
<xs:schema id="DataSetReader" targetNamespace="http://www.tempuri.org/DataSetReader.xsd" xmlns:mstns="http://www.tempuri.org/DataSetReader.xsd" xmlns="http://www.tempuri.org/DataSetReader.xsd" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata" attributeFormDefault="qualified" elementFormDefault="qualified">
  <xs:element name="DataSetReader" msdata:IsDataSet="true" msdata:Locale="zh-CN">
    <xs:complexType>
      <xs:choice maxOccurs="unbounded">
        <xs:element name="Reader">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="ReaderID" type="xs:string" />
              <xs:element name="ReaderName" type="xs:string" minOccurs="0" />
              <xs:element name="ReaderPassword" type="xs:string" minOccurs="0" />
              <xs:element name="ReaderPhoneNo" type="xs:string" minOccurs="0" />
              <xs:element name="ReaderEmail" type="xs:string" minOccurs="0" />
              <xs:element name="ReaderBorrowedbooks" type="xs:int" minOccurs="0" />
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:choice>
    </xs:complexType>
    <xs:unique name="Constraint1" msdata:PrimaryKey="true">
      <xs:selector xpath=".//mstns:Reader" />
      <xs:field xpath="mstns:ReaderID" />
    </xs:unique>
  </xs:element>
</xs:schema>